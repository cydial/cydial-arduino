    Serial.println("[[L13]]");
    Serial.println(R[1].value);
    Serial.println("[[L12-Right]]");
    Serial.println(R[0].value);
    delay(DEBUG_INTERVAL);
  }
  for(i=0; i<=6; i++)
  {
      if(L[maxindex].value <= L[i].value)
      {
          maxindex = i;
      }
  }
  for(i=0; i<=6; i++)
  {
      if(R[maxindex2].value <= R[i].value)
      {
